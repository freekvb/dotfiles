{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0c0a0a90",
                "editor.background": "#0c0a0a90",
                "editor.foreground": "#c2c1c1",
                "text": "#c2c1c1",
                "text.muted": "#c2c1c170",
                "text.ignored": "#c2c1c140",
                "text.placeholder": "#c2c1c150",
                "ignored": "#c2c1c130",
                "element.hover": "#c2c1c130",
                "ghost_element.hover": "#635b5130",
                "ghost_element.selected": "#635b5130",
                "ghost_element.active": "#635b5160",
                "border": "#0c0a0a",
                "editor.highlighted_line.background": "#635b5110",
                "editor.active_line.background": "#635b5110",
                "panel.background": "#0c0a0a90",
                "title_bar.background": "#0c0a0a90",
                "title_bar.inactive_background": "#0c0a0a90",
                "status_bar.background": "#0c0a0a90",
                "drop_target.background": "#0c0a0a90",
                "elevated_surface.background": "#0c0a0a",
                "toolbar.background": "#0c0a0a90",
                "tab_bar.background": "#0c0a0a90",
                "tab.inactive_background": "#0c0a0a90",
                "tab.active_background": "#635b5130",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0c0a0a",
                "scrollbar.thumb.background": "#635b51",
                "editor.gutter.background": "#0c0a0a90",
                "terminal.background": "#0c0a0a10",
                "terminal.foreground": "#c2c1c1",
                "terminal.dim_foreground": "#c2c1c1",
                "terminal.bright_foreground": "#c2c1c1",
                "terminal.ansi.black": "#0c0a0a",
                "terminal.ansi.red": "#635b51",
                "terminal.ansi.green": "#6b645a",
                "terminal.ansi.yellow": "#81786e",
                "terminal.ansi.blue": "#948a7f",
                "terminal.ansi.magenta": "#9e9389",
                "terminal.ansi.cyan": "#a59d96",
                "terminal.ansi.white": "#968a8a",
                "terminal.ansi.bright_black": "#695656",
                "terminal.ansi.bright_red": "#847A6C",
                "terminal.ansi.bright_green": "#8F8678",
                "terminal.ansi.bright_yellow": "#ACA193",
                "terminal.ansi.bright_blue": "#C6B9AA",
                "terminal.ansi.bright_magenta": "#D3C5B7",
                "terminal.ansi.bright_cyan": "#DCD2C8",
                "terminal.ansi.bright_white": "#c2c1c1",
                "modified": "#635b51",
                "syntax": {
                    "attribute": {
                        "color": "#635b51"
                    },
                    "boolean": {
                      "color": "#6b645a"
                    },
                    "comment": {
                      "color": "#695656"
                    },
                    "comment.doc": {
                      "color": "#695656"
                    },
                    "constant": {
                      "color": "#8F8678"
                    },
                    "function": {
                      "color": "#DCD2C8"
                    },
                    "keyword": {
                      "color": "#948a7f"
                    },
                    "number": {
                      "color": "#9e9389"
                    },
                    "operator": {
                      "color": "#948a7f"
                    },
                    "string": {
                      "color": "#635b51"
                    },
                    "variable": {
                      "color": "#6b645a"
                    }
                },
                "players": [
                  {
                    "cursor": "#635b51",
                    "background": "#0c0a0a",
                    "selection": "#635b5130"
                  }
                ]
            }
        }
    ]
}
